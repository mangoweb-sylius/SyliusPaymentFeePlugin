<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<services>
		<defaults public="true"/>

		<service id="Tests\MangoSylius\PaymentFeePlugin\Behat\Pages\Admin\PaymentMethod\UpdatePageInterface"
				 class="Tests\MangoSylius\PaymentFeePlugin\Behat\Pages\Admin\PaymentMethod\UpdatePage"
				 parent="sylius.behat.page.admin.crud.update" public="false">
			<argument type="string">sylius_admin_channel_update</argument>
		</service>

		<service id="sylius.behat.context.ui.admin.payment_method_with_fee"
				 class="Tests\MangoSylius\PaymentFeePlugin\Behat\Context\Ui\Admin\ManagingAdminPaymentMethodWithExtraFeeContext">
			<argument type="service" id="Tests\MangoSylius\PaymentFeePlugin\Behat\Pages\Admin\PaymentMethod\UpdatePageInterface"/>
		</service>

	</services>
</container>
